# DTM gRPC 协议端口号
DTM_GRPC_PORT=36790


# ETCD ###########################################
# Etcd 服务映射宿主机端口号，可在宿主机127.0.0.1:1004
ETCD_PORT=2379

ETCD_MANAGE_PORT=1004

# PROMETHEUS #####################################
# Prometheus 服务映射宿主机端口号，可在宿主机127.0.0.1:1005访问
